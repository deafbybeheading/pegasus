<?xml version="1.0" encoding="utf-8" standalone="no"?>
<application xmlns="http://ns.adobe.com/air/application/2.0" minimumPatchLevel="0">
  <!-- AIR Application Descriptor File. See http://www.adobe.com/go/learn_air_1.0_application_descriptor_en. -->
  <id>org.postgresql.pgconsole.PGConsole</id>
  <name>PGConsole</name>
  <version>1.0</version>
  <filename>PGConsole-@VERSION@</filename>
  <description>
    <text xml:lang="en">This is a demo application for Pegasus, an ActionScript driver for PostgreSQL.</text>
  </description>
  <copyright>Copyright 2009-2011, PostgreSQL Global Development Group</copyright>
  <initialWindow>
    <content>PGConsole-@VERSION@.swf</content>
    <title>PGConsole: A Pegasus Demo</title>
    <systemChrome>standard</systemChrome>
    <transparent>false</transparent>
    <visible>true</visible>
    <minimizable>true</minimizable>
    <maximizable>true</maximizable>
    <resizable>true</resizable>
    <width>1100</width>
    <height>500</height>
    <x>150</x>
    <y>150</y>
    <minSize>550 400</minSize>
    <maxSize>1600 1000</maxSize>
  </initialWindow>
  <installFolder>pegasus/pgconsole</installFolder>
  <programMenuFolder>pegasus/pgconsole</programMenuFolder>
  
  <customUpdateUI>false</customUpdateUI>
  <allowBrowserInvocation>false</allowBrowserInvocation>

</application>